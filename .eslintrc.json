{
    "env": {
        "commonjs": true,
        "es2015": true,
        "node": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {

      "no-console": "off",
      "camelcase": "off",
      "import/no-dynamic-require": "off",
      "no-unused-vars": "off"
    }
}
