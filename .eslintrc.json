{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
    },
    "rules": {
      "no-console": "off",
      "camelcase": "off",
      "import/no-dynamic-require": "off",
      "no-unused-vars": "off"
    }
}
